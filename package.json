    "jest": "^23.6.0",
    "vue-jest": "^3.0.1",
    "webpack-dev-server": "2.11.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "roots": [
